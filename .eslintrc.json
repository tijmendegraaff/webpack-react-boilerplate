{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import", "jest"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "no-underscore-dangle": [0]
  }
}
